<div class="account-dashboard">
    <div class="container">
        <div class="row">
            <div class="col-sm-12 col-md-3 col-lg-3">
                
            </div>
          <!-- Assuming the EJS template is named orderDetails.ejs -->

<div class="col-sm-12 col-md-9 col-lg-9">
  <!-- Order Details Content -->
  <div class="tab-content dashboard_content" data-aos="fade-up" data-aos-delay="200">
      <div class="tab-pane fade show active" id="order-details">
          <h3>Order Details</h3>
          <div class="order-details-container">
              <% if (locals.orderedData) { %> <!-- Check if orderedData exists -->
                  <div class="order-info">
                      <p><strong>Order ID:</strong> <%= orderedData._id %></p>
                      <p><strong>Order Date:</strong> <%= new Date(orderedData.orderDate).toLocaleDateString('en-US', { year: 'numeric', month: 'long', day: 'numeric' }) %></p>
                      <p><strong>Total Amount:</strong> $<%= orderedData.totalAmount %></p>
                      <p><strong>Order Status:</strong> <%= orderedData.orderStatus %></p>
                      <p><strong>Payment Method:</strong> <%= orderedData.paymentMethod %></p>
                  </div>

                  <div class="order-products">
                      <h4>Ordered Products</h4>
                      <table class="styled-table">
                          <thead>
                              <tr>
                                  <th>Product</th>
                                  <th>Quantity</th>
                                  <th>Size</th>
                              </tr>
                          </thead>
                          <tbody>
                            <% if (orderedData.products && orderedData.products.length > 0) { %>
                              <% for (let i = 0; i < orderedData.products.length; i++) { %>
                                <tr>
                                  <% if (orderedData.products[i].productId) { %>
                                    <td><%= orderedData.products[i].productId.productName %></td>
                                  <% } else { %>
                                    <td>Product not found</td>
                                  <% } %>
                                  <td><%= orderedData.products[i].quantity %></td>
                                  <td><%= orderedData.products[i].size %></td>
                                </tr>
                              <% } %>
                            <% } else { %>
                              <tr>
                                <td colspan="3">No products found in this order.</td>
                              </tr>
                            <% } %>
                            
                          </tbody>
                          
                          
                      </table>
                  </div>

                  <div class="order-address">
                      <h4>Shipping Address</h4>
                      <% if (orderedData.address) { %>
                          <p><strong>Name:</strong> <%= orderedData.address.addressName %></p>
                          <p><strong>Mobile:</strong> <%= orderedData.address.mobile %></p>
                          <p><strong>Home Address:</strong> <%= orderedData.address.homeAddress %></p>
                          <p><strong>City:</strong> <%= orderedData.address.city %></p>
                          <p><strong>District:</strong> <%= orderedData.address.district %></p>
                          <p><strong>State:</strong> <%= orderedData.address.state %></p>
                          <p><strong>Pincode:</strong> <%= orderedData.address.pincode %></p>
                      <% } else { %>
                          <p>Address information is not available.</p>
                      <% } %>
                  </div>
              <% } else { %>
                  <p>No order details found.</p> <!-- Display message if no order data is available -->
              <% } %>
          </div>
      </div>
  </div>
</div>


          
          <!-- End of col -->
