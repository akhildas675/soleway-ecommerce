        <style>
            .address-container,
            .billing-address,
            .tab-pane p {
                color: rgb(57, 57, 57);
            }

            .address-container {
                display: grid;
                grid-template-columns: repeat(auto-fill, minmax(250px, 1fr));
                /* Adjust the min-width as needed */
                gap: 20px;
                /* Space between columns */
            }

            .address-item {
                border: 1px solid #ddd;
                /* Optional: Add border to address item */
                padding: 15px;
                /* Optional: Add padding */
                background-image: linear-gradient(-180deg, rgba(255, 255, 255, 0.50) 0%, rgba(0, 0, 0, 0.50) 100%);
                background-blend-mode: lighten;
                /* Optional: Background color */
                border-radius: 10px;
            }

            .view {
                display: inline-block;
                margin-bottom: 10px;
                color: #007bff;
                text-decoration: none;
            }

            .view:hover {
                text-decoration: underline;
            }

            /* Ensure the table container is responsive */
            .table_page {
                overflow-x: auto;
                -webkit-overflow-scrolling: touch;
                /* Adds smooth scrolling on iOS */
                margin-bottom: 20px;
                width: 100%;
            }

            /* Style the table to be fully responsive */
            .table_page table {
                width: 100%;
                border-collapse: collapse;
            }

            .table_page th,
            .table_page td {
                padding: 12px 15px;
                text-align: left;
                white-space: nowrap;
                /* Prevents text from wrapping */
                border-bottom: 1px solid #dddddd;
                /* Add border for better visibility */
            }

            .table_page th {
                background-color: #f2f2f2;
                font-weight: bold;
            }

            .table_page tbody tr:hover {
                background-color: #f5f5f5;
                /* Add a hover effect for rows */
            }

            .table_page .success {
                color: green;
                /* Customize the color for the status */
            }

            /* Add some styling for the action button */
            .table_page .view {
                color: #007bff;
                text-decoration: none;
                font-weight: bold;
            }

            .table_page .view:hover {
                text-decoration: underline;
            }

            .wallet-section {
                display: flex;
                align-items: flex-start;
                /* Align the items to the top */
                gap: 20px;
                /* Space between the wallet box and aside */
            }

            .wallet-box {
                width: 300px;
                /* Medium size width */
                padding: 20px;
                border: 1px solid #c7c7c7;
                border-radius: 8px;
                background-image: linear-gradient(-180deg, rgba(255, 255, 255, 0.5) 0%, rgba(0, 0, 0, 0.50) 100%);
                background-blend-mode: lighten;
                /* Gradient background */
                text-align: center;
                box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
                /* Add a subtle shadow */
                color: rgb(75, 75, 75);
                /* Text color for contrast */
            }

            .money-input {
                width: 100%;
                /* Full width inside the wallet box */
                padding: 8px;
                margin: 10px 0;
                border: 1px solid #6f6f6f;
                border-radius: 5px;
                color: #000000;
                /* Text color for the input */
            }

            .add-money-btn {
                padding: 10px 20px;
                background-color: #000000;
                color: white;
                border: none;
                border-radius: 5px;
                cursor: pointer;
            }

            .add-money-btn:hover {
                background-color: #7e7e7e;
            }

            .aside-bar {
                width: 200px;
                /* Adjust as per the size of your aside */
                padding: 20px;
                background-color: #efefef;
                border-radius: 8px;
            }

            .coupon-container {
                display: grid;
                grid-template-columns: repeat(2, 1fr);
                /* 2 columns per row */
                gap: 20px;
                margin-top: 20px;
            }

            .coupon-box {
                background-image: linear-gradient(-180deg, rgba(255, 255, 255, 0.50) 0%, rgba(0, 0, 0, 0.50) 100%);
                background-blend-mode: lighten;

                border: 1px solid #ddd;
                border-radius: 8px;
                padding: 10px;
                /* Reduced padding */
                box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
                display: flex;
                flex-direction: column;
                justify-content: space-between;
                width: 60%;
                /* Ensures box takes up full width */
            }

            .coupon-details h5 {
                font-size: 20px;
                /* Smaller font size */
                font-weight: 600;
                margin-bottom: 8px;
                color: #5f5f5f;
            }

            .coupon-details p {
                font-size: 12px;
                /* Smaller font size */
                color: #323232;
                margin-bottom: 5px;
            }

            .danger {
                color: red;
                font-weight: bold;
            }

            .coupon-action {
                text-align: right;
            }

            .btn-copy {
                background-color: #000000;
                color: white;
                padding: 6px 12px;
                /* Smaller button */
                border: none;
                border-radius: 4px;
                cursor: pointer;
                transition: background-color 0.3s;
            }

            .btn-copy:hover {
                background-color: #3f3f3f;
            }

            @media (max-width: 768px) {
                .coupon-container {
                    grid-template-columns: 1fr;
                    /* 1 column for smaller screens */
                }
            }

            .order-list-container {
                display: grid;
                grid-template-columns: repeat(3, 1fr);
                /* 3 columns per row */
                gap: 20px;
                /* Adds space between each box */
                margin-bottom: 20px;
            }

            .order-box {
                width: 100%;
                /* Now it fits within the grid column */
                border: 1px solid #ddd;
                padding: 10px;
                border-radius: 8px;
                background-image: linear-gradient(-180deg, rgba(255, 255, 255, 0.50) 0%, rgba(0, 0, 0, 0.50) 100%);
                background-blend-mode: lighten;
                background-image: linear-gradient(-180deg, rgba(255, 255, 255, 0.50) 0%, rgba(0, 0, 0, 0.50) 100%);

            }

            .order-box-header {
                margin-bottom: 10px;
            }

            .order-box-content {
                display: flex;
                flex-wrap: wrap;
                align-items: flex-start;
                /* Aligns items at the top */
            }

            .product-info {
                display: flex;
                width: 100%;
                /* Ensure the product info takes up full width for better layout */
                border-bottom: 1px solid #f1f1f1;
                padding-bottom: 10px;
                align-items: flex-start;
                /* Aligns content to the top */
            }

            .product-image-container {
                width: 100px;
                /* Set a fixed width for the image container */
                height: 100px;
                /* Set a fixed height for the image container */
                display: block;
                /* Removed flexbox to align left */
                float: left;
                /* Align image to the left */
                background-color: #ffffff;
                border: 1px solid #ffffff;
                margin-right: 20px;
            }

            .centered-product-image {
                max-width: 100%;
                max-height: 100%;
                object-fit: contain;
                display: block;
            }

            .product-details {
                flex: 1;
                text-align: left;
                /* Align text content to the left */
                margin-left: 10px;
                /* Add a small margin for spacing */
            }

            .order-box-footer {
                text-align: right;
                margin-top: 10px;
            }

            .btn-primary {
                background-color: #000000;
                color: #fff;
                padding: 6px 10px;
                border: none;
                border-radius: 4px;
                text-decoration: none;
                cursor: pointer;
            }

            .btn-primary:hover {
                background-color: #262626;
            }

            /* Responsive adjustments for smaller screens */
            @media (max-width: 768px) {
                .order-list-container {
                    grid-template-columns: repeat(2, 1fr);
                    /* 2 columns on smaller screens */
                }
            }

            @media (max-width: 480px) {
                .order-list-container {
                    grid-template-columns: 1fr;
                    /* Single column on very small screens */
                }
            }

            .pagination {
                display: flex;
                justify-content: center;
                padding-left: 0;
                list-style: none;
                border-radius: 0.25rem;
            }

            .page-item {
                margin: 0 5px;
            }

            .page-item .page-link {
                position: relative;
                display: block;
                padding: 0.5rem 0.75rem;
                margin-left: -1px;
                line-height: 1.25;
                color: #000000;
                background-color: #fff;
                border: 1px solid #dee2e6;
                text-decoration: none;
                border-radius: 0.25rem;
                transition: background-color 0.2s ease, color 0.2s ease;
            }

            .page-item .page-link:hover {
                background-color: #000000;
                color: white;
                border-color: #000000;
            }

            .page-item.active .page-link {
                z-index: 1;
                color: #fff;
                background-color: #000000;
                border-color: #000000;
            }

            .page-item.disabled .page-link {
                color: #6c757d;
                pointer-events: none;
                background-color: #fff;
                border-color: #dee2e6;
            }

            .page-link {
                cursor: pointer;
            }

            .page-link:focus {
                outline: none;
                box-shadow: none;
            }

            .status-cancelled {
                color: red;
            }

            .status-placed {
                color: green;
            }
        </style>


</head>

